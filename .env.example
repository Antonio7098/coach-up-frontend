# Coach Up Frontend (.env.example)
# Copy to .env.local and fill with real values or set in Vercel

# Clerk (Auth)
CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Convex
CONVEX_DEPLOYMENT=
CONVEX_URL=

# AI Service (used by Next.js API routes)
AI_API_BASE_URL=http://127.0.0.1:8001
# Optionally expose to the client (not required for server-only routes)
NEXT_PUBLIC_AI_API_BASE_URL=

# Public config (safe to expose via NEXT_PUBLIC_*)
NEXT_PUBLIC_APP_ENV=local

# Rate limiting (optional placeholders)
RATE_LIMIT_MAX_RPS=10
RATE_LIMIT_BURST=20

# Logging
LOG_LEVEL=info
